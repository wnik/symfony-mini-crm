{% extends 'dashboard/index.html.twig' %}
{% block title %}
    {{ title }}
{% endblock %}
{% block content %}
    <div class="page-edit">
        <div class="header">
            <h1>New invoice</h1>
        </div>
        <div class="edit-form">
            {{ form_start(form, { attr: { class: 'col-lg-6 col-md-6 col-sm-6 col-xs-12 row' } }) }}
            {{ form_widget(form) }}
            {{ form_end(form) }}
            <div class="clear"></div>
        </div>
    </div>
    <div class="page-edit padding-top">
        <div class="header">
            <h1>Basket</h1>
        </div>
        <div class="edit-form">

            <div class="clear"></div>
        </div>
    </div>
{% endblock %}